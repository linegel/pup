{
  "name": "pup",
  "private": true,
  "scripts": {
    "start": "meteor --settings settings-development.json",
    "staging": "DEPLOY_HOSTNAME=us-east-1.galaxy-deploy.meteor.com meteor deploy <staging-subdomain>.meteorapp.com --settings settings-staging.json",
    "production": "DEPLOY_HOSTNAME=us-east-1.galaxy-deploy.meteor.com meteor deploy <production-subdomain>.meteorapp.com --settings settings-production.json",
    "test": "jest"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.4",
    "@cleverbeagle/seeder": "^2.0.3",
    "b64-to-blob": "^1.2.19",
    "babel-runtime": "^6.26.0",
    "commonmark": "^0.29.0",
    "core-js": "^3.4.2",
    "file-saver": "^2.0.2",
    "fsevents": "^2.1.2",
    "handlebars": "^4.5.3",
    "jquery": "^3.4.1",
    "jquery-validation": "^1.19.1",
    "jszip": "^3.2.2",
    "juice": "^5.2.0",
    "lodash": "^4.17.15",
    "meteor-node-stubs": "^0.4.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "polished": "^3.4.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-addons-pure-render-mixin": "^15.6.2",
    "react-autobind": "^1.0.6",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-markdown": "^4.2.2",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "simpl-schema": "^1.5.6",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "babel-jest": "^24.9.0",
    "babel-plugin-lodash": "^3.3.4",
    "enzyme": "^3.10.0",
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-meteor": "^6.0.0",
    "eslint-plugin-react": "^7.16.0",
    "jest": "^24.9.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "meteor",
      "react"
    ],
    "extends": [
      "airbnb",
      "plugin:meteor/recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "browser": true
    },
    "globals": {
      "expect": false
    },
    "rules": {
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 0,
      "import/extensions": 0,
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "to"
          ]
        }
      ],
      "max-len": 0,
      "no-alert": 0,
      "no-console": 0,
      "no-restricted-globals": 0,
      "no-return-assign": [
        "error",
        "except-parens"
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_id",
            "_ensureIndex",
            "_verifyEmailToken",
            "_resetPasswordToken",
            "_name"
          ]
        }
      ],
      "class-methods-use-this": 0,
      "react/jsx-filename-extension": 0,
      "react/forbid-prop-types": 0
    },
    "settings": {
      "import/resolver": "meteor"
    }
  },
  "version": "1.5.0",
  "description": "A boilerplate for products.",
  "main": "index.js",
  "repository": {
    "url": "git@cleverbeagle:cleverbeagle/pup.git",
    "type": "git"
  },
  "author": "cleverbeagle <ryan.glover@cleverbeagle.com>",
  "license": "MIT"
}
